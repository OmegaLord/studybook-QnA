{
  "extends": [
    "jquery",
    "airbnb"
  ],
  "globals": {
    "document": true,
    "window": true,
    "$": true,
    "jQuery": true,
    "App": true,
    "gon": true
  },
  "env": {
    "es6": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "rules": {
    "max-len": [ 1, 120 ]
  }
}
